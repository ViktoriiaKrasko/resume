<hr class='my-5' />

<div class='row m-4'>
  {{#each footer}}
    <div class='col-2'>
      {{#each this}}
        <div class='nav flex-column'>
          <div class='nav-item'>
            <a
              href='{{link}}'
              class='btn btn-link'
            >{{text}}</a>
          </div>
        </div>
      {{/each}}
      <div class='nav-item'>
        <a
          href='#'
          class='btn btn-link disabled'
        >Disabled</a>
      </div>
    </div>
  {{/each}}
</div>