{{! Це контейнер для сторінки Program }}

<head>
  <style type='text/css'>
    #background-image{width: 100%; height: 100%; position:
    fixed; left: 0px; top: 0px; z-index: -1; opacity: 0.3}
    #wrapper{font-family: Verdana, sans-serif; display:
    block; text-align: center; width: 60%; margin: 0 auto;}
    h2, h3 {color:rgb(83, 5, 156);} .headphones {height:
    20px; width: 20px;} ul li {list-style-type: none;}

  </style>
</head>
<body>

  <button style='float: right; font-size: 2rem;'>
    <a href='/main'>Main</a></button>
  <img
    src='https://upload.wikimedia.org/wikipedia/commons/thumb/0/06/Louvre_Museum%2CParis%2CFrance.jpg/1280px-Louvre_Museum%2CParis%2CFrance.jpg'
    alt='image of Eiffel Tower'
    id='background-image'
  />
  <main id='wrapper'>
    {{#with program.excursion}}
      <h1>{{name}}</h1>
      {{#with location}}
        <h2>{{city}}, {{country}}</h2>
      {{/with}}
      <p><b>Date: {{date}}</b></p>
      <h3>Tour Program:</h3>
      {{#each program}}
        <ul>
          <li><strong><em>{{name}}</em></strong>
            -
            {{type}}. Duration:
            {{duration}}
            hours.
            <br />
            <br />
            <details>
              {{#with details.museum}}
                <em>{{name}}</em>
                <br />
                {{#with location}}
                  {{address}},
                  {{city}},
                  {{country}}
                {{/with}}
                <br />
                <br />
                <u>Exhibits:</u>
                {{#each exhibits}}
                  <ul>
                    <li><em>{{name}}</em>
                      {{#if artist}}
                        by
                        {{artist}}
                      {{/if}}
                      <br />
                      <span>{{description}}</span>
                      <br />
                      {{#if audio_guide}}
                        Included audio-guide
                        <img
                          class='headphones'
                          src='data:image/jpeg;base64,'
                          alt='headphones'
                        />
                      {{/if}}
                    </li>
                  </ul>
                {{/each}}
              {{/with}}
              {{#with details.venue}}
                <em>{{name}}</em>
                <br />
                {{#with location}}
                  {{address}},
                  {{city}},
                  {{country}}
                {{/with}}
              {{/with}}
              <br />
              <br />
              {{#each details.performers}}
                <em>{{name}}</em>
                <br />
                {{type}}
                <br />
                <br />
              {{/each}}
              {{#with details.guide}}
                <p>You guide: {{name}}</p>
                <p>Language: {{language}}</p>
                <p>Rating: {{rating}}</p>

              {{/with}}
            </details>
          </li>
        </ul>
      {{/each}}
    {{/with}}
  </main>
</body>